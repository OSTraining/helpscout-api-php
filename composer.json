{
    "name": "ostraining/helpscout-api-php",
    "type": "library",
    "description": "API client for the Help Scout API",
    "keywords": ["api", "api-client", "helpscout"],
    "homepage": "http://www.helpscout.net",
    "license": "MIT",
    "repositories": [
        {
            "url": "https://github.com/Nanigans/curl.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-curl": "*",
        "Nanigans/curl": "master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-0": { "HelpScout\\": "src/" }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
